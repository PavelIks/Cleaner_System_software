<Window x:Class="Cleaner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cleaner"
        mc:Ignorable="d"
        Title="Cleaner" Height="450" Width="800" WindowStyle="SingleBorderWindow" Closing="closing">
    
    <Grid>
        <Button Margin="10,10,580,359" Click="showlogs">Показать log-файлы</Button>
        <ListBox Name="eventList" Margin="217,10,10,10" SelectionChanged="chng"></ListBox>
        <Button Margin="10,65,580,304" Content="Убить все процессы" Click="process_kill"/>
        <Button Margin="10,120,580,249" Content="Очистить папку «Загрузки»" Click="down_del"/>
        <Button Margin="10,175,580,194" Content="Очистить папку «temp»" Click="tmp_del"/>
    </Grid>
    
</Window>